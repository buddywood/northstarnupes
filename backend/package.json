{
  "name": "1kappa-backend",
  "version": "1.0.0",
  "description": "Express.js backend for 1Kappa",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "lint": "echo 'Linting skipped - ESLint not configured'",
    "type-check": "tsc --noEmit",
    "migrate": "tsx src/db/migrations.ts",
    "reset:db": "tsx src/scripts/reset-db.ts",
    "verify:db": "tsx src/scripts/verify-db-setup.ts",
    "verify:roles": "tsx src/scripts/verify-roles.ts",
    "seed": "tsx src/db/seed.ts",
    "seed:base": "tsx src/db/seed-base.ts",
    "seed:chapters": "tsx src/scripts/seed-chapters.ts",
    "seed:alumni": "tsx src/scripts/seed-alumni-chapters.ts",
    "seed:products": "tsx src/scripts/seed-products.ts",
    "seed:industries": "tsx src/scripts/seed-industries.ts",
    "update:provinces": "tsx src/scripts/update-provinces.ts",
    "seed:test": "tsx src/db/seed-test.ts",
    "seed:test:clear": "tsx src/db/seed-test.ts --clear",
    "seed:attributes": "tsx src/scripts/seed-product-attributes.ts",
    "seed:notifications": "tsx src/scripts/seed-notifications.ts",
    "seed:test-users": "tsx src/scripts/seed-test-users.ts",
    "seed:verification-test": "tsx src/scripts/seed-verification-test.ts",
    "seed:verification-test:clear": "tsx src/scripts/seed-verification-test.ts --clear",
    "verify:members": "tsx src/scripts/verify-members.ts",
    "verify:members:watch": "tsx src/scripts/verify-members.ts --visible",
    "verify:members:debug": "tsx src/scripts/verify-members.ts --visible --debug",
    "verify:members:record": "tsx src/scripts/verify-members.ts --visible --record",
    "grant-admin": "tsx src/scripts/grant-admin-access.ts",
    "approve-member": "tsx src/scripts/approve-member.ts",
    "update-cognito-emails": "AWS_PROFILE=1kappa_user tsx src/scripts/update-cognito-email-templates.ts",
    "check-cognito-user": "tsx src/scripts/check-cognito-user-status.ts",
    "fix:orphaned-member": "tsx src/scripts/fix-orphaned-member.ts",
    "delete:seller": "tsx src/scripts/delete-seller.ts",
    "delete:promoter": "tsx src/scripts/delete-promoter.ts",
    "delete:steward": "tsx src/scripts/delete-steward.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:stripe:integration": "STRIPE_INTEGRATION_TEST=true jest stripe-integration.test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.490.0",
    "@aws-sdk/client-location": "^3.929.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-ses": "^3.929.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@types/image-size": "^0.7.0",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "image-size": "^2.0.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "puppeteer": "^24.15.0",
    "stripe": "^14.9.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  }
}
